{
    "name": "php-ddd/php-ddd",
    "description": "Example of structure and implementation of DDD in php with Symfony and Doctrine",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "",
            "email": "email@example.com"
        }
    ],
    "require": {
        "php": "^5.5",
        "behat/behat": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Example\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Example\\": ["tests", "features/bootstrap"]
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
